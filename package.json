    "prettier-plugin-tailwindcss": "^0.5.6",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-sequential-preprocessor": "^2.0.1",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
