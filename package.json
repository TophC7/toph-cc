    "@fortawesome/svelte-fontawesome": "^0.2.0",
    "@neoconfetti/svelte": "^1.0.0",
    "@poppanator/sveltekit-svg": "^4.1.3",
    "@skeletonlabs/skeleton": "^2.3.0",
    "@skeletonlabs/tw-plugin": "^0.2.2",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.27.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/cookie": "^0.5.1",
